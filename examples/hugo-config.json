{
  "$schema": "../config-schema.json",
  "id": "hugo-test/master",
  "mode": "hugo",
  "source": {
    "remote": {
      "branch": "master",
      "repository": "https://github.com/gohugoio/hugoDocs.git",
      "checkout_path": "./data/hugo-test/build"
    }
  },
  "build": {
    "hugo": {
      "version": "0.51",
      "extended": false
    }
  },
  "publish": {
